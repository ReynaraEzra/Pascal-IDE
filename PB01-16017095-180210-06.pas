Program BanyakDigit;
uses crt;
var
//Deklarasi Variabel
        X:string;
        Y:integer;
begin
//Algoritma
        write('Masukan X        : ');
        readln(X);
        Y:=length(X);
        write(X);
        write(' Terdiri Dari ');
        write(Y);
        write(' Angka ');
        readln;
end.
//Program telah selesai

